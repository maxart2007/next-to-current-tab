{
  "manifest_version": 3,
  "name": "Next to Current Tab",
  "version": "1.0.0",
  "description": "Always open new tabs next to the current active tab",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Next to Current Tab"
  }
}

